[user]
	name = Jackson Dean
	email = jckson.dean@gmail.com
[alias]
	ap = add --patch
	cam = commit --amend
	co = checkout
	cob = checkout -b
	l = log
	rbu = review update --update-desc
	st = status
  b = !git checkout $(git branch -a | fzf)
  g = log --graph --oneline --all --decorate
  sub = !git review dcommit && git submit --async
  wip = !git add -A && git commit -m 'WIP' -n
[color]
	ui = true
[core]
	excludesfile = /Users/jcdean/.gitignore_global
	editor = code --wait
